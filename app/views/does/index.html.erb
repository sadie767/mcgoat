<h1>Show Does</h1>
<div class="row">
  <% @does.each do |doe| %>
  <div class="col-md-4">
    <%= image_tag('doe.jpg', :width =>'150') %>
    <li><%= link_to doe.name, do_path(doe) %></li>
    <li><%= number_to_currency doe.price %></li>
    </div>
  <% end %>
</div>
<h3><%= link_to 'New Doe', new_do_path %></h3>
